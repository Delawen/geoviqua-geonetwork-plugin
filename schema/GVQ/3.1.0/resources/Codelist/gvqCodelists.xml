					<gml:identifier codeSpace="GeoViQua">journalArticle</gml:identifier>
				</gmx:CodeDefinition>
			</gmx:codeEntry>
			<gmx:codeEntry>
				<gmx:CodeDefinition gml:id="GVQ_PublicationCategoryCode_magazineNewspaper">
					<gml:description>article in lay media (print or online)</gml:description>
					<gml:identifier codeSpace="GeoViQua">magazineNewspaper</gml:identifier>
				</gmx:CodeDefinition>
			</gmx:codeEntry>
            <gmx:codeEntry>
				<gmx:CodeDefinition gml:id="GVQ_PublicationCategoryCode_atlasPaperMap">
					<gml:description>Atlas or paper map</gml:description>
					<gml:identifier codeSpace="GeoViQua">atlasPaperMap</gml:identifier>
				</gmx:CodeDefinition>
			</gmx:codeEntry>
			<gmx:codeEntry>
				<gmx:CodeDefinition gml:id="GVQ_PublicationCategoryCode_applicationProgram">
					<gml:description>Software application or program</gml:description>
					<gml:identifier codeSpace="GeoViQua">applicationProgram</gml:identifier>
				</gmx:CodeDefinition>
			</gmx:codeEntry>
			<gmx:codeEntry>
				<gmx:CodeDefinition gml:id="GVQ_PublicationCategoryCode_workflow">
					<gml:description>Published workflow, e.g. Taverna workflow on MyExperiment</gml:description>
					<gml:identifier codeSpace="GeoViQua">workflow</gml:identifier>
				</gmx:CodeDefinition>
			</gmx:codeEntry>
			<gmx:codeEntry>
				<gmx:CodeDefinition gml:id="GVQ_PublicationCategoryCode_conferenceProceedings">
					<gml:description>article in conference proceedings (print or online)</gml:description>
					<gml:identifier codeSpace="GeoViQua">conferenceProceedings</gml:identifier>
				</gmx:CodeDefinition>
			</gmx:codeEntry>
            <gmx:codeEntry>
				<gmx:CodeDefinition gml:id="GVQ_PublicationCategoryCode_cdDvd">
					<gml:description>CD or DVD</gml:description>
					<gml:identifier codeSpace="GeoViQua">cdDvd</gml:identifier>
				</gmx:CodeDefinition>
			</gmx:codeEntry>
			<gmx:codeEntry>
				<gmx:CodeDefinition gml:id="GVQ_PublicationCategoryCode_blogWiki">
					<gml:description>Blog, wiki or other Web 2.0 content</gml:description>
					<gml:identifier codeSpace="GeoViQua">blogWiki</gml:identifier>
				</gmx:CodeDefinition>
			</gmx:codeEntry>
			<gmx:codeEntry>
				<gmx:CodeDefinition gml:id="GVQ_PublicationCategoryCode_webSite">
					<gml:description>Full website dedicated to the topic</gml:description>
					<gml:identifier codeSpace="GeoViQua">webSite</gml:identifier>
				</gmx:CodeDefinition>
			</gmx:codeEntry>
            			<gmx:codeEntry>
				<gmx:CodeDefinition gml:id="GVQ_PublicationCategoryCode_webPage">
					<gml:description>web page dedicated to the topic</gml:description>
					<gml:identifier codeSpace="GeoViQua">webPage</gml:identifier>
				</gmx:CodeDefinition>
			</gmx:codeEntry>
            <gmx:codeEntry>
				<gmx:CodeDefinition gml:id="GVQ_PublicationCategoryCode_onlineVideo">
					<gml:description>Online video - for example, YouTube commentary</gml:description>
					<gml:identifier codeSpace="GeoViQua">onlineVideo</gml:identifier>
				</gmx:CodeDefinition>
			</gmx:codeEntry>
		</gmx:CodeListDictionary>
	</gmx:codelistItem>
	<!--=== GVQ_ReportAspectCode ===-->
	<gmx:codelistItem>
		<gmx:CodeListDictionary gml:id="GVQ_ReportAspectCode">
			<gml:description>mode in which the data is represented</gml:description>
			<gml:identifier codeSpace="GeoViQua">GVQ_ReportAspectCode</gml:identifier>
			<gmx:codeEntry>
				<gmx:CodeDefinition gml:id="GVQ_ReportAspectCode_usage">
					<gml:description>report about usage of a dataset</gml:description>
					<gml:identifier codeSpace="GeoViQua">usage</gml:identifier>
				</gmx:CodeDefinition>
			</gmx:codeEntry>
			<gmx:codeEntry>
				<gmx:CodeDefinition gml:id="GVQ_ReportAspectCode_problem">
					<gml:description>report about a problem identified with a dataset</gml:description>
					<gml:identifier codeSpace="GeoViQua">problem</gml:identifier>
				</gmx:CodeDefinition>
			</gmx:codeEntry>
			<gmx:codeEntry>
				<gmx:CodeDefinition gml:id="GVQ_ReportAspectCode_fitnessForPurpose">
					<gml:description>report on fitness-for-purpose of a dataset in a specified context (defined by applicationDomain and usageDescription)</gml:description>
					<gml:identifier codeSpace="GeoViQua">fitnessForPurpose</gml:identifier>
				</gmx:CodeDefinition>
			</gmx:codeEntry>
			<gmx:codeEntry>
				<gmx:CodeDefinition gml:id="GVQ_ReportAspectCode_alternatives">
					<gml:description>identification of alternative datasets to the one referenced</gml:description>
					<gml:identifier codeSpace="GeoViQua">alternatives</gml:identifier>
				</gmx:CodeDefinition>
			</gmx:codeEntry>
		</gmx:CodeListDictionary>
	</gmx:codelistItem>
	<!--=== CI_RoleCode ===-->
	<gmx:codelistItem>
		<gmx:CodeListDictionary gml:id="GVQ_UserRoleCode">
			<gml:description>Role in which the user is reporting on a dataset: NB, an individual may have multiple roles atdifferent times, depending on their usage of data</gml:description>
			<gml:identifier codeSpace="GeoViQua">GVQ_UserRoleCode</gml:identifier>
			<gmx:codeEntry>
				<gmx:CodeDefinition gml:id="GVQ_UserRoleCode_commercialDataProducer">
					<gml:description>Commercial producer</gml:description>
					<gml:identifier codeSpace="GeoViQua">commercialDataProducer</gml:identifier>
				</gmx:CodeDefinition>
			</gmx:codeEntry>
			<gmx:codeEntry>
				<gmx:CodeDefinition gml:id="GVQ_UserRoleCode_researchEndUser">
					<gml:description>Research end user</gml:description>
					<gml:identifier codeSpace="GeoViQua">researchEndUser</gml:identifier>
				</gmx:CodeDefinition>
			</gmx:codeEntry>
			<gmx:codeEntry>
				<gmx:CodeDefinition gml:id="GVQ_UserRoleCode_nonResearchEndUser">
					<gml:description>End user without specific research or science priorities</gml:description>
					<gml:identifier codeSpace="GeoViQua">nonResearchEndUser</gml:identifier>
				</gmx:CodeDefinition>
			</gmx:codeEntry>
			<gmx:codeEntry>
				<gmx:CodeDefinition gml:id="GVQ_UserRoleCode_scientificDataProducer">
					<gml:description>Individual or institution producing data as a part of the scientific process but without primary commercial intent; for example, gathering research data, generating derived products</gml:description>
					<gml:identifier codeSpace="GeoViQua">scientificDataProducer</gml:identifier>
				</gmx:CodeDefinition>
			</gmx:codeEntry>
		</gmx:CodeListDictionary>
	</gmx:codelistItem>
    <!--=== MD_ScopeCode ===-->
	<gmx:codelistItem>
		<gmx:CodeListDictionary gml:id="MD_ScopeCode">
			<gml:description>class of information to which the referencing entity applies</gml:description>
			<gml:identifier codeSpace="GeoViQua">MD_ScopeCode</gml:identifier>
			<gmx:codeEntry>
				<gmx:CodeDefinition gml:id="MD_ScopeCode_document">
					<gml:description>information applies to a document (e.g., report, publication)</gml:description>
					<gml:identifier codeSpace="GeoViQua">document</gml:identifier>
				</gmx:CodeDefinition>
			</gmx:codeEntry>
			<gmx:codeEntry>
				<gmx:CodeDefinition gml:id="MD_ScopeCode_metadataDocument">
					<gml:description>information applies to a metadata document</gml:description>
					<gml:identifier codeSpace="GeoViQua">metadataDocument</gml:identifier>
				</gmx:CodeDefinition>
			</gmx:codeEntry>
            <gmx:codeEntry>
				<gmx:CodeDefinition gml:id="MD_ScopeCode_activity">
					<gml:description>information applies to an activity, project or program</gml:description>
					<gml:identifier codeSpace="GeoViQua">activity</gml:identifier>
				</gmx:CodeDefinition>
			</gmx:codeEntry>
            <gmx:codeEntry>
				<gmx:CodeDefinition gml:id="MD_ScopeCode_attribute">
					<gml:description>information applies to the attribute class</gml:description>
					<gml:identifier codeSpace="GeoViQua">attribute</gml:identifier>
				</gmx:CodeDefinition>
			</gmx:codeEntry>
			<gmx:codeEntry>
				<gmx:CodeDefinition gml:id="MD_ScopeCode_attributeType">
					<gml:description>information applies to the characteristic of a feature</gml:description>
					<gml:identifier codeSpace="GeoViQua">attributeType</gml:identifier>
				</gmx:CodeDefinition>
			</gmx:codeEntry>
			<gmx:codeEntry>
				<gmx:CodeDefinition gml:id="MD_ScopeCode_collectionHardware">
					<gml:description>information applies to the collection hardware class</gml:description>
					<gml:identifier codeSpace="GeoViQua">collectionHardware</gml:identifier>
				</gmx:CodeDefinition>
			</gmx:codeEntry>
			<gmx:codeEntry>
				<gmx:CodeDefinition gml:id="MD_ScopeCode_collectionSession">
					<gml:description>information applies to the collection session</gml:description>
					<gml:identifier codeSpace="GeoViQua">collectionSession</gml:identifier>
				</gmx:CodeDefinition>
			</gmx:codeEntry>
			<gmx:codeEntry>
				<gmx:CodeDefinition gml:id="MD_ScopeCode_dataset">
					<gml:description>information applies to the dataset</gml:description>
					<gml:identifier codeSpace="GeoViQua">dataset</gml:identifier>
				</gmx:CodeDefinition>
			</gmx:codeEntry>
			<gmx:codeEntry>
				<gmx:CodeDefinition gml:id="MD_ScopeCode_series">
					<gml:description>information applies to the series</gml:description>
					<gml:identifier codeSpace="GeoViQua">series</gml:identifier>
				</gmx:CodeDefinition>
			</gmx:codeEntry>
			<gmx:codeEntry>
				<gmx:CodeDefinition gml:id="MD_ScopeCode_nonGeographicDataset">
					<gml:description>information applies to non-geographic data</gml:description>
					<gml:identifier codeSpace="GeoViQua">nonGeographicDataset</gml:identifier>
				</gmx:CodeDefinition>
			</gmx:codeEntry>
			<gmx:codeEntry>
				<gmx:CodeDefinition gml:id="MD_ScopeCode_dimensionGroup">
					<gml:description>information applies to a dimension group</gml:description>
					<gml:identifier codeSpace="GeoViQua">dimensionGroup</gml:identifier>
				</gmx:CodeDefinition>
			</gmx:codeEntry>
			<gmx:codeEntry>
				<gmx:CodeDefinition gml:id="MD_ScopeCode_feature">
					<gml:description>information applies to a feature</gml:description>
					<gml:identifier codeSpace="GeoViQua">feature</gml:identifier>
				</gmx:CodeDefinition>
			</gmx:codeEntry>
			<gmx:codeEntry>
				<gmx:CodeDefinition gml:id="MD_ScopeCode_featureType">
					<gml:description>information applies to a feature type</gml:description>
					<gml:identifier codeSpace="GeoViQua">featureType</gml:identifier>
				</gmx:CodeDefinition>
			</gmx:codeEntry>
			<gmx:codeEntry>
				<gmx:CodeDefinition gml:id="MD_ScopeCode_propertyType">
					<gml:description>information applies to a property type</gml:description>
					<gml:identifier codeSpace="GeoViQua">propertyType</gml:identifier>
				</gmx:CodeDefinition>
			</gmx:codeEntry>
			<gmx:codeEntry>
				<gmx:CodeDefinition gml:id="MD_ScopeCode_fieldSession">
					<gml:description>information applies to a field session</gml:description>
					<gml:identifier codeSpace="GeoViQua">fieldSession</gml:identifier>
				</gmx:CodeDefinition>
			</gmx:codeEntry>
			<gmx:codeEntry>
