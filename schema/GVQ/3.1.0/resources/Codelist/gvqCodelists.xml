<?xml version="1.0" encoding="UTF-8"?>
<!-- 
Adaptation of gmx codelists to GeoViQua codelists
-->
<gmx:CT_CodelistCatalogue xmlns="http://www.geoviqua.org/QualityInformationModel/3.1" 
xmlns:gmx="http://www.isotc211.org/2005/gmx" xmlns:gco="http://www.isotc211.org/2005/gco" 
xmlns:gml="http://www.opengis.net/gml" xmlns:xlink="http://www.w3.org/1999/xlink" 
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xsi:schemaLocation="
http://www.isotc211.org/2005/gmx http://schemas.opengis.net/iso/19139/20070417/gmx/gmx.xsd 
http://www.isotc211.org/2005/gco http://schemas.opengis.net/iso/19139/20070417/gco/gco.xsd">
	<!--=====Catalogue description=====-->
	<gmx:name>
		<gco:CharacterString>gvqCodelists</gco:CharacterString>
	</gmx:name>
	<gmx:scope>
		<gco:CharacterString>Codelists for description of quality elements in metadata datasets compliant with ISO/TC 211 19115:2003 and 19139, and with the GeoViQua Producer Quality Model</gco:CharacterString>
	</gmx:scope>
	<gmx:fieldOfApplication>
		<gco:CharacterString>GVQ (and imported) namespace</gco:CharacterString>
	</gmx:fieldOfApplication>
	<gmx:versionNumber>
		<gco:CharacterString>0.0</gco:CharacterString>
	</gmx:versionNumber>
	<gmx:versionDate>
		<gco:Date>2012-05-02</gco:Date>
	</gmx:versionDate>
	<!--============================================================================-->
	<!--============================================================================-->
	<!--============================= Codelists =======================================-->
	<!--=== GVQ_PublicationPurposeCode ===-->
	<gmx:codelistItem>
		<gmx:CodeListDictionary gml:id="GVQ_PublicationPurposeCode">
			<gml:description>identification of when a given event occurred</gml:description>
			<gml:identifier codeSpace="GeoViQua">GVQ_PublicationPurposeCode</gml:identifier>
			<gmx:codeEntry>
				<gmx:CodeDefinition gml:id="GVQ_PublicationPurposeCode_compare">
					<gml:description>Compares the dataset with others. RelatedResource can indicate the compared dataset's identifier</gml:description>
					<gml:identifier codeSpace="GeoViQua">compare</gml:identifier>
				</gmx:CodeDefinition>
			</gmx:codeEntry>
			<gmx:codeEntry>
				<gmx:CodeDefinition gml:id="GVQ_PublicationPurposeCode_derive">
					<gml:description>Derives a new dataset from this one. RelatedResource can indicate the derived dataset's identifier</gml:description>
					<gml:identifier codeSpace="GeoViQua">derive</gml:identifier>
				</gmx:CodeDefinition>
			</gmx:codeEntry>
			<gmx:codeEntry>
				<gmx:CodeDefinition gml:id="GVQ_PublicationPurposeCode_describe">
					<gml:description>Describes the dataset</gml:description>
					<gml:identifier codeSpace="GeoViQua">describe</gml:identifier>
				</gmx:CodeDefinition>
			</gmx:codeEntry>
            <gmx:codeEntry>
				<gmx:CodeDefinition gml:id="GVQ_PublicationPurposeCode_evaluate">
					<gml:description>Evaluates the dataset. including its quality. This may also be used by producers to append publications on CAL-VAL results</gml:description>
					<gml:identifier codeSpace="GeoViQua">evaluate</gml:identifier>
				</gmx:CodeDefinition>
			</gmx:codeEntry>
		</gmx:CodeListDictionary>
	</gmx:codelistItem>
	<!--=== GVQ_PublicationCategoryCode ===-->
	<gmx:codelistItem>
		<gmx:CodeListDictionary gml:id="GVQ_PublicationCategoryCode">
			<gml:description>Category of publication, medium in which it is published</gml:description>
			<gml:identifier codeSpace="GeoViQua">GVQ_PublicationCategoryCode</gml:identifier>
			<gmx:codeEntry>
				<gmx:CodeDefinition gml:id="GVQ_PublicationCategoryCode_bookChapter">
					<gml:description>book chapter</gml:description>
					<gml:identifier codeSpace="GeoViQua">bookChapter</gml:identifier>
				</gmx:CodeDefinition>
			</gmx:codeEntry>
			<gmx:codeEntry>
				<gmx:CodeDefinition gml:id="GVQ_PublicationCategoryCode_book">
					<gml:description>book</gml:description>
					<gml:identifier codeSpace="GeoViQua">book</gml:identifier>
				</gmx:CodeDefinition>
			</gmx:codeEntry>
			<gmx:codeEntry>
				<gmx:CodeDefinition gml:id="GVQ_PublicationCategoryCode_reportManual">
