<!-- 
	Describes conversions to other schemas 

		 The name is the name of the converter - used as a service name if a 
		 service is provided to convert records from search results or show (this
		 name should be unique in GeoNetwork so prefixing with xml_<schema_name>
		 is a good idea.

     The xslt attribute is the name of an xslt that does the conversion
		 in the convert dir of the schema 
-->
<conversions>
	<!-- GeoViQua specific -->
	<converter name="xml_iso19139.gvq" nsUri="http://www.geoviqua.org/QualityInformationModel/4.0"
		schemaLocation="http://schemas.geoviqua.org/GVQ/4.0/GeoViQua_PQM_UQM.xsd" xslt="xml_iso19139.gvq.xsl"/>
	<converter name="xml_iso19139.gvqTo19139" nsUri="http://www.isotc211.org/2005/gmd"
		schemaLocation="www.isotc211.org/2005/gmd/gmd.xsd" xslt="to19139.xsl"/>
	<converter name="xml_iso19139.gvqTooai_dc" nsUri="http://www.openarchives.org/OAI/2.0/"
		schemaLocation="http://www.openarchives.org/OAI/2.0/oai_dc.xsd" xslt="oai_dc.xsl"/>
</conversions>
